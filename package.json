  "license": "MIT",
  "author": {
    "name": "Russ Biggs",
    "url": "https://russbiggs.com"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-css": "node-sass --include-path scss src/scss/main.scss  dist/style.css --output-style compressed",
    "build": "webpack",
    "watch": "webpack --watch",
    "deploy": "yarn run clean && yarn run build-css && yarn run build && cp index.html ./dist/index.html && cp data.json ./dist/data.json && cp -a static/. ./dist/"
  },
  "dependencies": {
    "mitt": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.1",
    "eslint": "^7.13.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}